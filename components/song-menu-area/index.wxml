<!--components/song-menu-area/index.wxml-->
<view class="area">
    <scroll-view scroll-x class="menu-list">
        <block wx:for="{{songMenu}}" wx:key="id">
            <view class="menu-item">
                <song-menu-item item="{{item}}"></song-menu-item>
            </view>
        </block>
    </scroll-view>
</view>
